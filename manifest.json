{
    "manifest_version": 3,
    "name": "AI Tracker",
    "version": "1.0",
    "description": "Analyzes your webtime usage using GPT-4 API",
    "author": "bentang8229@gmail.com",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png"
        },
        "default_title": "AI Tracker",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": ["js/config.js", "js/urls.js", "js/fn.js", "js/storage-local.js", "background.js"]
    },
    "offline_enabled": true,
    "permissions": [
        "tabs",
        "idle",
        "storage",
        "unlimitedStorage"
    ]
}